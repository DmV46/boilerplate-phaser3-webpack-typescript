{
  "name": "boilerplate-phaser3-webpack-typescript",
  "version": "1.0.0",
  "description": "A Phaser 3 project template with ES6 support via [Webpack 5](https://webpack.js.org/) that includes hot-reloading for development and production-ready builds.",
  "main": "src/index.ts",
  "scripts": {
    "build": "webpack --env mode=production --config webpack.config.ts",
    "start": "webpack serve -- --env mode=development port=3011 --config webpack.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"{config,src}/**/*.{ts,tsx,js}\"",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "20.10.5",
    "@types/webpack": "5.28.5",
    "@types/webpack-dev-server": "4.7.2",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.2",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "prettier": "3.1.1",
    "raw-loader": "4.0.2",
    "terser-webpack-plugin": "5.3.9",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.10.0"
  },
  "dependencies": {
    "phaser": "3.60.0"
  }
}
